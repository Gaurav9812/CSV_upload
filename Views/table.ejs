
<link rel="stylesheet" type="text/css" href="//cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css">
<link rel="stylesheet" type="text/css" href="/css/table.css">
<div id="outer-div">
   
<p style="text-align: end;margin: 1rem;"><a href="/"><button style="width: 4rem;height: 2rem;">Home</button></a></p>
    <h2 style="text-align: center;"><%=name%></h2>

    <table border="2" class="table table-success table-striped">

    <thead>
        <% var keys=keys[0].split(',')%>
        
        <!-- <%#For Dynamic HEaders%> -->
    <% for(let i=0;i<keys.length;i++){%>
        <th ><%=keys[i]%></th>
    

    <%}%>
    </thead>
    <tbody>
        <!-- <%#Iterating over all parsed file and adding to table%> -->
    <% for(let i=1;i<results.length;i++){%>
        <%var ans=results[i][0].split(',')%>
        <tr>
        <% for(let j=0;j<keys.length;j++){%>

            <td><%=ans[j]%></td>
            
            <%}%>
        </tr>
        <%}%>
    </tbody>

</table>
</div>
<script src="//cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
<script src="/js/table.js"></script>