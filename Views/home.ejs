    <link rel="stylesheet" href="/css/home.css">
    <h1>CSV Upload</h1>
    <form enctype="multipart/form-data" method="post" action="/create" id="form1" >
        <input type="file" name="file" id="file"><br/>
        <button type="submit" id="Upload" >Upload File</button>
    </form>

    <div id="outer-div1">
        <div id="outer-div2">
            <h2>uploaded-files</h2>
            
            <%for(let i=0;i<Files.length;i++){%>
                
                <p class="uploaded-files">
                    <a href="/View/?filePath=<%=Files[i].File%>"><%=Files[i].Name%></a>
                    <a href="/delete/?filePath=<%=Files[i].File%>">
                        <i class="fas fa-trash"> </i></a>
                </p>
            <%}%>
        </div>
    </div>
    <script src="/js/home.js"></script>